plugin.tx_nodelinkvisualizer_pi1 {
    view {
        templateRootPaths.0 = EXT:node_link_visualizer/Resources/Private/Templates/
        partialRootPaths.0 = EXT:node_link_visualizer/Resources/Private/Partials/
        layoutRootPaths.0 = EXT:node_link_visualizer/Resources/Private/Layouts/
    }
    persistence {
        storagePid = {$plugin.tx_nodelinkvisualizer_pi1.persistence.storagePid}
    }
}

page {
    includeJSFooter {
        d3js = https://d3js.org/d3.v7.min.js
        d3js.external = 1
        nodelinkvisualizer = EXT:node_link_visualizer/Resources/Public/JavaScript/NodeLinkVisualizer.js
    }
}

module.tx_nodelinkvisualizer_pi1 < plugin.tx_nodelinkvisualizer_pi1